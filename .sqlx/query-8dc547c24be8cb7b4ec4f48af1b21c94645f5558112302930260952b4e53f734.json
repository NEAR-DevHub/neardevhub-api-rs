{
  "db_name": "PostgreSQL",
  "query": "\n            WITH latest_snapshots AS (\n                SELECT DISTINCT ON (id) *\n                FROM dao_proposals\n                WHERE \n                    dao_instance = $1 AND\n                    (LOWER(description) LIKE $2 OR\n                    LOWER(hash) LIKE $2)\n                ORDER BY id, submission_time DESC\n            )\n            SELECT *\n            FROM latest_snapshots\n            ORDER BY id DESC\n            LIMIT 10\n            ",
