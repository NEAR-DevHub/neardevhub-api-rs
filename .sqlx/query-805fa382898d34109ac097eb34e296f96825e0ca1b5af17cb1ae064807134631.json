  "db_name": "PostgreSQL",
  "query": "\n            WITH latest_snapshots AS (\n                SELECT DISTINCT ON (id) *\n                FROM dao_proposals\n                WHERE \n                    LOWER(description) LIKE $1 OR\n                    LOWER(hash) LIKE $1\n                ORDER BY id, submission_time DESC\n            )\n            SELECT *\n            FROM latest_snapshots\n            ORDER BY id DESC\n            LIMIT 10\n            ",
