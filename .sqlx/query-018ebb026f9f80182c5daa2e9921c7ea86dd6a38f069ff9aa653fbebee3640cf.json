{
  "db_name": "PostgreSQL",
  "query": "\n          SELECT\n            ps.proposal_id,\n            p.author_id,\n            ps.block_height,\n            ps.ts,\n            ps.editor_id,\n            ps.social_db_post_block_height,\n            ps.labels,\n            ps.proposal_version,\n            ps.proposal_body_version,\n            ps.name,\n            ps.category,\n            ps.summary,\n            ps.description,\n            ps.linked_proposals,\n            ps.linked_rfp,\n            ps.requested_sponsorship_usd_amount,\n            ps.requested_sponsorship_paid_in_currency,\n            ps.requested_sponsor,\n            ps.receiver_account,\n            ps.supervisor,\n            ps.timeline,\n            ps.views\n          FROM\n            proposals p\n            INNER JOIN (\n              SELECT\n                proposal_id,\n                MAX(ts) AS max_ts\n              FROM\n                proposal_snapshots\n              GROUP BY\n                proposal_id\n            ) latest_snapshots ON p.id = latest_snapshots.proposal_id\n            INNER JOIN proposal_snapshots ps ON latest_snapshots.proposal_id = ps.proposal_id\n            AND latest_snapshots.max_ts = ps.ts;\n          ",
