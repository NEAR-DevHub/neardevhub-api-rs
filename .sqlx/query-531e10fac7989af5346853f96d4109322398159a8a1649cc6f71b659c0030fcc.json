{
  "db_name": "PostgreSQL",
  "query": "\n            WITH latest_snapshots AS (\n                SELECT DISTINCT ON (proposal_id, dao_instance) *\n                FROM dao_proposals\n                WHERE \n                    LOWER(description) LIKE $1 OR\n                    LOWER(hash) LIKE $1\n                ORDER BY proposal_id, dao_instance, submission_time DESC\n            )\n            SELECT *\n            FROM latest_snapshots\n            ORDER BY proposal_id DESC\n            LIMIT $2\n            OFFSET $3\n            ",
